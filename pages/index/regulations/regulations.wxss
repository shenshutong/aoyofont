@import "../../../commpents/navbar.wxss";

page {
    width: 100%;
    background-color: #f6f6f6;
}

.box {
    width: 100%;
    height: 50rpx;
    padding: 30rpx;
    box-sizing: border-box;
}

.item {
    width: 100%;
    background-color: #ffff;
    box-shadow: 0px 18rpx 42rpx 0px rgba(91, 91, 91, 0.11);
    border-radius: 40rpx;
    padding: 15rpx;
    box-sizing: border-box;
    margin-bottom: 30rpx;
    display: flex;
    flex-flow: column;
    justify-content: center;
    padding-left: 20rpx;
    box-sizing: border-box;
}
.item view{
    margin-bottom: 10rpx;
    font-size: 26rpx;
}