page {
    width: 100%;
    height: 100%;
    position: relative;
    overflow: hidden;
}

.login {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
}

.login1 {
    width: 100%;
    height: 160rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 1;
}

.nowGoIndex {
    width: 300rpx;
    height: 90rpx;
    background-color: red;
    position: absolute;
    bottom: 20%;
    left: 50%;
    z-index: 999;
    margin-left: -150rpx;
    opacity: 0;
}

.skp {
    z-index: 9999;
    width: 205rpx;
    height: 50rpx;
    background-color: #000;
    border-radius: 50rpx;
    position: absolute;
    top: 4%;
    left: 22%;
    z-index: 999;
    margin-left: -150rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 20rpx;
    opacity: .5;
    color: #fff;
}